{
  "version": "2.0.0",
  "tasks": [
    {
      "label": "prelaunch:native",
      "type": "npm",
      "script": "prelaunch:native",
      "group": {
        "kind": "build",
        "isDefault": true
      },
      "problemMatcher": []
    },
    {
      "label": "prelaunch:web",
      "type": "npm",
      "script": "prelaunch:web",
      "group": {
        "kind": "build"
      },
      "problemMatcher": []
    },
    {
      "type": "npm",
      "script": "watch:web",
      "group": "build",
      "isBackground": true,
      "problemMatcher": "$tsc-watch"
    }
  ]
}
