{
  "name": "ldproxy-editor",
  "displayName": "ldproxy for VS Code",
  "description": "ldproxy configuration editor",
  "version": "1.1.2",
  "private": true,
  "license": "MIT",
  "repository": "https://github.com/ldproxy/editor",
  "publisher": "iide",
  "preview": true,
  "categories": [
    "Linters",
    "Formatters",
    "Other"
  ],
  "keywords": [
    "ldproxy",
    "configuration",
    "editor",
    "generator"
  ],
  "engines": {
    "vscode": "^1.82.2"
  },
  "icon": "./icon.png",
  "activationEvents": [
    "onStartupFinished"
  ],
  "contributes": {
    "commands": [
      {
        "command": "ldproxy-editor.showAutoCreate",
        "title": "Create new entities",
        "category": "ldproxy",
        "icon": "$(add)"
      },
      {
        "command": "ldproxy-editor.showAutoCreateValues",
        "title": "Create new values",
        "category": "ldproxy",
        "icon": "$(add)"
      }
    ],
    "configuration": {
      "type": "object",
      "title": "ldproxy Editor Configuration",
      "properties": {
        "ldproxy-editor.baseUrl": {
          "type": "string",
          "format": "uri",
          "scope": "machine"
        }
      }
    },
    "capabilities": {
      "untrustedWorkspaces": {
        "supported": true,
        "restricted": false
      }
    }
  },
  "browser": "./extension.js"
}
